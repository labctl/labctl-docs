{
    "eslint.run": "onType",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "volar.tsPlugin": true,
    "cSpell.words": [
      "containerlab",
      "dblclick",
      "golang",
      "labctl",
      "tmpl",
      "topo",
      "vars",
      "vitepress"
    ],
    "cSpell.ignorePaths": [
      ".git",
      ".vscode",
      "node_modules",
      "package.json",
      "package-lock.json",
      ".prettierrc",
      ".eslintrc.js"
    ],
    "[typescript]": {
      "editor.tabSize": 4,
      "editor.defaultFormatter": "esbenp.prettier-vscode",
      "editor.formatOnSave": true
    },
    "[vue]": {
      "editor.tabSize": 4,
      "editor.defaultFormatter": "esbenp.prettier-vscode",
      "editor.formatOnSave": true
    },
    "[json]": {
      "editor.tabSize": 4,
      "editor.defaultFormatter": "esbenp.prettier-vscode",
      "editor.formatOnSave": true
    },
    "cSpell.ignoreWords": [

    ]
  }